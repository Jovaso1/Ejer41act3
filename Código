    private void botoncalcularActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // TODO add your handling code here:
        double dat1, dat2, mayor;
        dat1= Double.parseDouble(txtdat1.getText());
        dat2= Double.parseDouble(txtdat2.getText());
        mayor= Elmayor.encontrarelmayor(dat1, dat2);
        
        txtmayor.setText(String.valueOf(mayor));
    }                                             

    private void botonborrarActionPerformed(java.awt.event.ActionEvent evt) {                                            
        txtdat1.setText("");
        txtdat2.setText("");
        txtmayor.setText("");
    }                                           

    private void botonsalirActionPerformed(java.awt.event.ActionEvent evt) {                                           
        dispose();
    }                                          

