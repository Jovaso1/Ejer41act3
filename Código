Clase elmayor:
package ejer41act3;
/**
 *
 * @author USER
 */
public class Elmayor {
    public static double encontrarelmayor(double dat1, double dat2){
    double mayor;
    mayor = Math.max(dat1,dat2);
    return mayor;
    }
    
}
